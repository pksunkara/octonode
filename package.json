{
  "name": "octonode",
  "description": "nodejs wrapper for github v3 api",
  "version": "0.1.0",
  "author": "Pavan Kumar Sunkara <pavan.sss1991@gmail.com> (http://pksunkara.github.com)",
  "contributors": [
    { "name": "Pavan Kumar Sunkara", "email": "pavan.sss1991@gmail.com" }
  ],
  "dependencies": {
    "request": "*"
  },
  "devDependencies": {
    "coffee-script": "1.1.2",
    "nock": "~0.7.0",
    "vows": "~0.6.0",
    "should": ">=0.2.1",
    "mocha": "0.7.x",
    "sinon": "1.2.x",
    "async" : "0.1.x",
    "underscore" : "1.2.x"
  },
  "keywords": ["wrapper", "api", "v3", "github"],
  "repository": {
    "type": "git",
    "url": "git://github.com/pksunkara/octonode.git"
  },
  "bugs": {
    "url": "http://github.com/pksunkara/octonode/issues"
  },
  
  "main": "./lib/octonode",
  "scripts": {
    "test_vows": "vows --spec $(find test -name '*-test.js')",
    "prepublish": "cake lib",
    "test": "NODE_ENV=test node_modules/.bin/mocha -R spec test/*.coffee",
    "watch": "node_modules/.bin/coffee -c -w  -o lib src"
    
  },
  "engines": { "node": ">0.4.11" },
  "licenses": [
    {
      "type": "MIT",
      "url": "http://github.com/pksunkara/octonode/raw/master/LICENSE"
    }
  ]
}
